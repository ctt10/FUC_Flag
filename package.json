{
  "name": "FUC-Flag",
  "description": "# This is an implementation of game -Place deployed on near blockchain",
  "license": "(MIT AND Apache-2.0)",
  "version": "0.0.1",
  "engines": {
    "node": "15.x",
    "yarn": "1.x"
  },
  "scripts": {
    "build": "asp --verbose",
    "dev:deploy:contract": "near dev-deploy --wasmFile=\"./build/release/main.wasm\"",
    "deploy:contract": "near deploy",
    "deploy": "yarn build && yarn deploy:contract && yarn deploy:pages",
    "prestart": "yarn dev && yarn deploy:contract",
    "start": "parcel src/index.html",
    "asp": "asp --verbose",
    "test": "yarn asp && yarn jest",
    "jest": "yarn build && jest test",
    "dev": "yarn build && yarn dev:deploy:contract && env-cmd -f ./neardev/dev-account.env parcel src/index.html"
  },
  "devDependencies": {
    "asp": "^0.0.2",
    "dotenv": "^8.2.0",
    "env-cmd": "^10.1.0",
    "jest": "^26.6.3",
    "near-cli": "^1.5.2",
    "asbuild": "^0.0.9",
    "near-sdk-as": "^2.2.2",
    "parcel-bundler": "^1.12.4"
  },
  "dependencies": {
    "assemblyscript":"^0.17.10",
    "@as-pect/assembly": "^5.0.1",
    "@as-pect/cli": "^5.0.0",
    "@as-pect/core": "^5.0.1",
    "@as-pect/csv-reporter": "^5.0.1",
    "@as-pect/json-reporter": "^5.0.1",
    "@as-pect/snapshots": "^5.0.1",
    "@assemblyscript/loader": "^0.9.4",
    "@babel/core":"^7.0.0-0",
    "@types/diff": "^4.0.2",
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.14",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "assemblyscript": "^0.17.10",
    "coveralls": "^3.1.0",
    "eslint": "^7.16.0",
    "jest": "^26.6.3",
    "lerna": "^3.22.1",
    "npm-run-all": "^4.1.5",
    "@octokit/core": "^3.2.4",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "strip-ansi": "^6.0.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "near-api-js": "^0.36.2",
    "regenerator-runtime": "^0.13.7"
  },
  "jest": {
    "testEnvironment": "near-cli/test_environment",
    "testPathIgnorePatterns": [
      "<rootDir>/assembly/",
      "<rootDir>/node_modules/"
    ]
  }
}
